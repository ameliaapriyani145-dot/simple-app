pipeline {
    agent any
    options {
        timeout(time: 30, unit: 'MINUTES')
        retry(3)
    }
    stages {
        stage('chekout') {
            steps {
                git 'https://github.com/ameliaapriyani145-dot/proyek-pertama'
            }
        }
        stage('Build') {
            steps {
                echo 'Building the project'
            }
        }
        stage('Deploy') {
            steps {
                echo'Deploy complete!'
            }
        }
    }
}